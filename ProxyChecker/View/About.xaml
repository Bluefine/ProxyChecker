<mah:MetroWindow x:Class="ProxyChecker.View.About"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProxyChecker.View"
        mc:Ignorable="d"
        xmlns:mah="clr-namespace:MahApps.Metro.Controls;assembly=MahApps.Metro"
        xmlns:iconPacks="http://metro.mahapps.com/winfx/xaml/iconpacks"
        Title="About" Height="350" Width="400">
    <StackPanel Background="#252525">
        <Label VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="26" Margin="0 0 0 0">
            <Image Source="../Images/logo.png" Height="200" Width="200"></Image>
        </Label>
        <Label VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="20" Content="Version: 1.0.0.0"/>
        <Label VerticalAlignment="Top" HorizontalAlignment="Center" FontSize="14" Content="Copyright © 2021 Bluefine"/>
    </StackPanel>
